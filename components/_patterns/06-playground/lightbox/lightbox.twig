{%- embed "@decorators/lightbox/lightbox.twig" %}
	{%- block content %}
		{%- for i in 0..2 %}
			{%- embed "@decorators/lightbox/lightbox-item.twig" with {
				source: "http://via.placeholder.com/1400x1400",
				url: "#",
				classes: [],
			}-%}
				{%- block content %}
					<img class="img-fluid mb-3" src="http://via.placeholder.com/150x150" alt="What's up!">
				{% endblock -%}
			{% endembed %}
		{% endfor -%}
	{% endblock -%}
{% endembed -%}